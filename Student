public  class Student extends Person {
     int course = 1;
     public Student (int num, String name, int course) {
         super(num, name);
         this.course = course;
     }
     void tell () {
        System.out.println(super.numberofstudent);
        System.out.println(super.name);
        System.out.println(course);
    }

}
